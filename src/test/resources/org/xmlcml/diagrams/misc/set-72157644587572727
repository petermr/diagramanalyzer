	var useSuccess = function (Y, status){
	/* when modules fail... */
	if (status && !status.success){
		/* if we haven't retried already ... */
		if (comboRetry === 0){
			comboRetry++;
			/* display a debug message */
			if (typeof console !== 'undefined' && console.warn){
				console.warn('There was an error loading client modules, retrying: ' + status.msg);
			}
			/* beacon retry */
			if (sendRequest){
				sendRequest('flickr.hermes.clientapp.moduleretry', 'client app module loading fail, retrying');
			}
			/* try loading the modules again */
			Y.use('client-app', 'photo-page-view', 'photo-models', 'person-models', 'person-relationship-models', 'photo-engagement-models', 'photo-stats-models', 'photo-privacy-models', 'photo-tags-models', 'tag-models', 'photo-autotags-models', 'autotag-models', 'photo-notes-models', 'note-models', 'photo-location-models', 'photo-head-meta-models', 'photo-contexts-models', 'favorite-models', 'gallery-models', 'groups-pool-models', 'set-models', 'photostream-models',  useSuccess
			);
		}else{
			/* or, throw an error */
			if (typeof console !== 'undefined' && console.error){
				console.error('There was an error loading client modules: ' + status.msg);
				/* beacon error */
				if (sendRequest){
					sendRequest('flickr.hermes.clientapp.modulefail', 'client app module loading fail');
				}
				beaconError('There was an error loading client modules: ' + status.msg);
				displayError();
			}
		}
	}else{
		/* or, load the client app */
		initFcn(Y);
	}
};
	
	//see the page-head-module-loading view for the definition of
	// `useSuccess`
	app.yui.use('client-app', 'photo-page-view', 'photo-models', 'person-models', 'person-relationship-models', 'photo-engagement-models', 'photo-stats-models', 'photo-privacy-models', 'photo-tags-models', 'tag-models', 'photo-autotags-models', 'autotag-models', 'photo-notes-models', 'note-models', 'photo-location-models', 'photo-head-meta-models', 'photo-contexts-models', 'favorite-models', 'gallery-models', 'groups-pool-models', 'set-models', 'photostream-models',  useSuccess
	);
</script>

<script type="text/javascript" charset="utf-8">
	(function() {
		window.pageTiming = {};
		
		window.registerFirstPhoto = function() {
			// console.log('default.hb registerFirstPhoto');
			if (!window.pageTiming.timeToFirstPhoto) {
				window.pageTiming.timeToFirstPhoto = new Date().getTime();
			}
			window.registerFirstPhoto = function () {};
		};

		var htmlTag = document.documentElement;
		htmlTag.className = htmlTag.className.replace('no-js', '');

		
		window.beaconError = function(msg, url, line) {
			var img = new Image();
			img.src = "/beacon_rb_jserror.gne?error=" + encodeURIComponent(msg) + '&line='+ line + '&url=' + url;
		}

		window.onerror = beaconError;

	})();
</script>

